<body>

    <!-- NAVIGATION BAR -->

    <header>  
        
        <div class="upper-nav">

            <div id="logo">
                <div id="logo-img">
                    <img src="Image\logo.png" alt=" ">
                </div>
                <h1>ZoroMart</h1>
            </div>
    
            <div class="servic-no">
